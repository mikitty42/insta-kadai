<h1>Post</h1>
<table>
  <tr>
    <th>タイトル</th>
    <th>内容</th>
  </tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.content %></td>
    <td><%= link_to '詳細', post_path(post.id) %></td>
    <td><%= link_to "編集", edit_post_path(post.id) %></td>
    <td><%= link_to '削除', post_path(post.id), method: :delete %></td>
  </tr>
<% end %>
</table>
<%= link_to 'New Post', new_post_path %>
